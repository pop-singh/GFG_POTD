class Solution
{
       static void swap(int m,int n,int [][]arr)
    {
          int temp=arr[m][n];
          arr[m][n]=arr[n][m];
          arr[n][m]=temp;
    }
    public void transpose(int n,int arr[][])
    {
     
     for(int i=0;i<arr.length;i++)
     {
        for(int j=0;j<=i;j++)
        {
           swap(i,j,arr);
        }
     }
}
}
